    "react-native-web": "~0.19.10",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "3.3.2",
    "ts-pattern": "^5.2.0",
    "zod": "^3.23.8",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
